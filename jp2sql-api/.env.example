# Google AI Studio (Gemini) API キー
GOOGLE_API_KEY=your_api_key_here

# PostgreSQL 接続（READ ONLYロール）
# 例: postgresql://readonly_user:password@localhost:5432/sampledb?sslmode=prefer
DATABASE_URL=postgresql://readonly_user:password@localhost:5432/sampledb

# LLM モデル名（必要に応じて更新）
GEMINI_MODEL=gemini-1.5-flash

# LLMの最大トークン、温度など（必要に応じて）
GEMINI_TEMPERATURE=0.2
GEMINI_MAX_OUTPUT_TOKENS=1024

# SQL の最大 LIMIT（上限キャップ）
MAX_LIMIT=1000

# 取得するスキーマ名（カンマ区切り／空ならpublicのみ）
SCHEMAS=public
